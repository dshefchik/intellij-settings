<application>
  <component name="RemoteServers">
    <remote-server name="Docker for Mac" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Users" />
              <option name="virtualMachinePath" value="/Users" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="~/" />
              <option name="virtualMachinePath" value="/root/workspace" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="/private/tmp/ruby" />
              <option name="virtualMachinePath" value="/usr/local/bundle" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/.aws" />
              <option name="virtualMachinePath" value="/root/.aws" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
  </component>
</application>